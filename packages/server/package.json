{
  "name": "gamedaim-server",
  "version": "0.0.1",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --exit-child --respawn src/app.ts",
    "start": "node dist/src/app.js",
    "build": "rimraf ./dist && tsc",
    "clean": "rimraf node_modules build dist",
    "clean:build": "rimraf build dist",
    "lint": "eslint .",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "tap --node-arg=--require=ts-node/register",
    "db:generate": "pnpx prisma generate",
    "db:seed": "pnpx prisma db seed",
    "db:migrate": "pnpx prisma migrate dev --name init && pnpm db:seed",
    "db:push": "pnpx prisma db push --skip-generate",
    "db:studio": "pnpx prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.271.0",
    "@aws-sdk/lib-storage": "^3.271.0",
    "@faker-js/faker": "^7.6.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/multipart": "^7.4.1",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.3.0",
    "@prisma/client": "^4.10.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "eslint": "8.34.0",
    "fastify": "^4.13.0",
    "fastify-zod": "^1.2.0",
    "nanoid": "3.3.4",
    "pino-pretty": "^9.2.0",
    "sharp": "^0.31.3",
    "slugify": "^1.6.5",
    "tap": "^16.3.4",
    "ts-mock-imports": "^1.3.8",
    "zod": "^3.20.6",
    "zod-to-json-schema": "^3.20.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.13.0",
    "@types/tap": "^15.0.8",
    "eslint-config": "workspace:*",
    "prisma": "^4.10.1",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "typescript-config": "workspace:*"
  }
}
